#include <iostream>//cut ve tek elementlerin sayi
using namespace std;

int main() {
    int n = 0, m = 0, k;
    int a[] = {1, 2, 3, 4, 5, 6};
    k = sizeof(a) / 4;
    for(int i = 0; i < k; i++){
        if(a[i] % 2 == 0){
            n++;
        }
        else{
            m++;
        }
    }
    cout << "Cut: " << n << endl;
    cout << "Tek: " << m;
    return 0;
}

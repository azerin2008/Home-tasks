#include <iostream>//b ve c arasindaki ededlerin cemi
using namespace std;

int sum (int b, int c){
    int s = 0;
    int a[10];
    cout <<"a[1] = ";
    cin >> a[0];
    for (int i = 1; i < 10; i++){
        cout << "a[" << i + 1 << "] = ";
        cin >> a[i];
    }
    for(int i = 0; i < 10; i++){
        if(b < a[i] && a[i] < c){
            s = s + a[i];
        }
    }
    return s;
}

int main() {
    int b, c;
    cout << "b = ";
    cin >> b;
    cout << "c = ";
    cin >> c;
    int result = sum(b, c);
    cout <<"b ve c arasindaki ededlerin cemi: " << result;
    return 0;
}

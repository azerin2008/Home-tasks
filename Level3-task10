#include <iostream>//Fibonacci
using namespace std;

int main() {
   int a = 0;
   int b = 1;
   int n;
   cout << "Natural eded daxil edin: ";
   cin >> n;
   while(a <= n & b <= n){
       cout << a << endl;
       cout << b << endl;
       a = a + b;
       b = b + a;
   }
   if(a <= n){
           cout << a << endl;
   }
    return 0;
}

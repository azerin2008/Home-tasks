#include <iostream>
#include <cmath>
using namespace std;

int main() {
    double C;
    double F;
    cout << "Temperatur daxil edin: C = ";
    cin >> C;
    if(cin.fail()){
        cout << "Temperatur duzgun deyil." << endl;
        return 1;
    }
    else
    F = (9 * C)/5 + 32;
    cout << F;
    return 0;
}

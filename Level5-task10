#include <iostream>//cut ve tek elementler
#include <cmath>
using namespace std;

int main() {
    int a[] = {1, 2, 3, 4, 5, 6};
    int n = sizeof (a) / 4;
    cout << "Cut: ";
    for(int i = 0; i < n; i++){
        if(a[i] % 2 == 0){
            cout << a[i] << ", ";
        }
    }
   cout << endl;
    cout << "Tek: ";
    for(int i = 0; i < n; i++){
        if(a[i] % 2 == 1){
           cout << a[i] << ", ";
        }
    }
    return 0;
}

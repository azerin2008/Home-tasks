#include <iostream>//Ortalamadan boyuk elementler
using namespace std;

int main() {
    int a[] = {1, 2, 3, 4, 5};
    int n, b, c;
    n = sizeof(a) / sizeof(a[0]);
    for(int i = 0; i < n; i++){
        b = b + a[i];
    }
    c = b / n;
    cout << "Ededi ortadan boyuk elementler: " << endl;
    for(int j = 0; j < n; j++){
        if(a[j] > c){
            cout << a[j] << ", ";
        }
    }
    return 0;
}

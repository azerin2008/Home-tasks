#include <iostream>//Ededin bolenleri
using namespace std;

int main() {
    int a;
    int b = 1;
    cout << "Natural eded daxil edin: ";
    cin >> a;
    if(a > 0){
    while(b <= a){
        if(a % b ==0){
            cout << b <<endl;
        }
        b = b + 1;
    }
    }
    else
    cout << "Error";
    return 0;
}

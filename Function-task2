#include <iostream>//Funksiya ile iki ededin boyuyu
using namespace std;

double max(double a, double b){
    double c;
    if(a > b){
        c = a;
    }
    else{
        c = b;
    }
    return c;
}

int main() {
  double x = 0, y = 0;
  cout << "2 eded daxil edin: " << endl;
  cin >> x >> y;
  double netice = max(x, y);
  cout << "Max: " << netice;
    return 0;
}

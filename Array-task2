#include <iostream>//massivdeki en kicik musbet eded
using namespace std;

void musbetKicik(int n, int a[]){
    int b = a[0];
    for(int i = 0; i < n; i++){
        if(a[i] > 0){
            if(a[i] < b){
                b = a[i];
            }
        }
    }
        cout << b;
}

int main() {
    int n = 8;
    int arr[] = {11, 0, -3, 2, 4, -6, 3, -2};
    musbetKicik (n, arr);

    return 0;
}

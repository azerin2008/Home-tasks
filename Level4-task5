#include <iostream>//a ededinin tam bolenleri sayi
using namespace std;

int main() {
    int a, n, b = 1;
    cout << "Natural eded daxil edin: a = ";
    cin >> a;
    if(a > 0){
    while(b <= a){
        if(a % b == 0){
            n++;
        }
        b++;
    }
    cout << a << " ededinin tam bolenleri sayi: " << 2*n;
    }
    return 0;
}

#include <iostream>
using namespace std;

void insertionSort (int a[], int n){
    int x, j;
    for(int i = 1; i < n; i++){
        x = a[i];
        j = i - 1;
    while(j >= 0 && a[j] > x){
        a[j + 1] = a[j];
        j--;
    }
    a[j + 1] = x;
    }
}
int main() {
    int arr[] = {3, 7, 1, 4, 9, 6, 1};
    int n = sizeof(arr) / sizeof(arr[0]);
    insertionSort(arr, n);
    cout << "Siralanan massiv: ";
    for (int i = 0; i < n; i++){
        cout << arr[i] << ", ";
    }
    return 0;
}

#include <iostream>
using namespace std;

int main() {
  int n;
  cout << "Ayin nomresi: ";
  cin >> n;
  if(n > 12 || n < 0){
      cout << "Yanlis nomre!";
  }
  else
    if(n == 12 || n == 1 || n == 2){
        cout << "Qis feslidir.";
    }
    else if(n == 3 || n == 4 || n == 5){
        cout << "Yaz feslidir.";
    }
    else if(n == 6 || n == 7 || n == 8){
        cout << "Yay feslidir.";
    }
    else if(n == 9 || n == 10 || n == 11){
    cout << "Payiz feslidir.";
    }
    return 0;
}

#include <iostream>//1000-den kicik polindrom ededler
using namespace std;

int main() {
   int a, b, c, d, e;
   for(a = 1; a <= 1000; a++){
       d = a;
       c = 0;
       e = a;
       while(e > 0){
       b = e % 10;
       c = c * 10 + b;
       e = e / 10;
       if(c == d){
           cout << d << ", ";
       }
       }
   }
    return 0;
}
